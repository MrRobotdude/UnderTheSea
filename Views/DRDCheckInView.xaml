             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UnderTheSea.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label x:Name="label" Content="Dining Room Check In" HorizontalAlignment="Left" Margin="84,0,0,0" VerticalAlignment="Top"/>
        <Button x:Name="checkInBtn" Content="Check In" HorizontalAlignment="Center" VerticalAlignment="Center" Width="114" Height="53" Click="checkInBtn_Click"/>
        <Label x:Name="WarningLbl" Content="" HorizontalAlignment="Left" Margin="84,202,0,0" VerticalAlignment="Top" Width="128"/>

    </Grid>
</UserControl>
